@model IEnumerable<denemproje.Models.Randevu>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AdiSoyadi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CepNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RandevuSaati)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YapilanIslem)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AdiSoyadi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CepNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RandevuSaati)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.YapilanIslem)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
